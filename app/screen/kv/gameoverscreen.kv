<GameOverScreen>:
    AsyncImage:
        source: 'icons/background.jpg'
        allow_stretch: True
        keep_ratio: False
        keep_data: True

    FloatLayout:
        Label:
            color: 0.4, 0.3, 0, 1
            font_size: 30
            size_hint: None, None
            center: root.width / 2, 3 * root.height / 4

        Label:
            color: 0, 0, 0, 1
            font_size: 30
            size_hint: None, None
            center: root.width / 2, 3 * root.height / 4 - 50

